	<entitygroup name="Mushrooms">
		<entity name="animalWildRedMushroom" prob="0.1"/>
		<entity name="animalWildPuffBallMushroom" prob="0.1"/>
		<entity name="animalWildPurpleMushroom" prob="0.1"/>
