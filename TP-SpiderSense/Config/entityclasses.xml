<configs>	
	<append xpath="/entity_classes/entity_class[@name='animalZombieBear']">
		<property name="TrackerIcon" value="ui_game_symbol_tracking_bear"/>
		<property name="NavObject" value="twitch_spawn,twitch_spawn_other,animaltracking_bear,clear_sleeper" />
	</append>
	
	<append xpath="/entity_classes/entity_class[@name='animalZombieDog']">
		<property name="TrackerIcon" value="ui_game_symbol_tracking_wolf"/>
		<property name="NavObject" value="twitch_spawn,twitch_spawn_other,animaltracking_wolf,clear_sleeper" />
	</append>
	
	<append xpath="/entity_classes/entity_class[@name='animalZombieVulture']">
		<property name="TrackerIcon" value="ui_game_symbol_enemy_dot"/>
		<property name="NavObject" value="twitch_spawn,twitch_spawn_other,animaltracking_hostile,clear_sleeper"></property>
	</append>	
	
	<append xpath="/entity_classes/entity_class[@name='zombieTemplateMale']">
		<property name="TrackerIcon" value="ui_game_symbol_enemy_dot"/>
		<property name="NavObject" value="twitch_spawn,twitch_spawn_other,animaltracking_hostile,clear_sleeper"></property>
	</append>
	
	<append xpath="/entity_classes/entity_class[@name='zombieDemolition']">
		<property name="TrackerIcon" value="ui_game_symbol_explosion"/>
	</append>
	
	<append xpath="/entity_classes/entity_class[contains(@name,'zombie') and(contains(@name,'Feral'))]">
		<property name="TrackerIcon" value="ui_game_symbol_run"/>
	</append>
	
	<append xpath="/entity_classes/entity_class[contains(@name,'zombie') and(contains(@name,'Radiated'))]">
		<property name="TrackerIcon" value="ui_game_symbol_zombie"/>
	</append>
	
	<append xpath="/entity_classes/entity_class[contains(@name,'zombie') and not(contains(@name,'animal')) and (contains(@name,'Radiated'))]/property[@name='Tags']/@value">,radiatedz</append>
	<append xpath="/entity_classes/entity_class[contains(@name,'zombie') and not(contains(@name,'animal')) and (contains(@name,'Feral'))]/property[@name='Tags']/@value">,feralz,radiatedz</append>
	<append xpath="/entity_classes/entity_class[contains(@name,'zombie') and not(contains(@name,'Feral')) and not(contains(@name,'animal')) and not(contains(@name,'Radiated'))]/property[@name='Tags']/@value">,simplez,feralz,radiatedz</append>
	
	<append xpath="/entity_classes/entity_class[@name='animalZombieBear']/property[@name='Tags']/@value">,animalz,simplez,feralz,radiatedz</append>
	<append xpath="/entity_classes/entity_class[@name='animalZombieDog']/property[@name='Tags']/@value">,animalz,simplez,feralz,radiatedz</append>
	<append xpath="/entity_classes/entity_class[@name='animalZombieVulture']/property[@name='Tags']/@value">,animalz,simplez,feralz,radiatedz</append>
	<append xpath="/entity_classes/entity_class[@name='animalZombieVultureRadiated']/property[@name='Tags']/@value">,radiatedz</append>
</configs>
