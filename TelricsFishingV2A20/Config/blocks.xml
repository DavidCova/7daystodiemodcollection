<!-- adds worms to dirts -->
<append xpath="/blocks/block[@name='terrDirt']">
	<drop event="Destroy" name="resourceBaitWorm" count="1" prob=".41"/>
</append>
<append xpath="/blocks/block[@name='terrForestGround']">
	<drop event="Destroy" name="resourceBaitWorm" count="1" prob=".41"/>
</append>
<append xpath="/blocks/block[@name='terrDesertGround']">
	<drop event="Destroy" name="resourceBaitWorm" count="1" prob=".11"/>
</append>
<append xpath="/blocks/block[@name='terrBurntForestGround']">
	<drop event="Destroy" name="resourceBaitWorm" count="1" prob=".11"/>
</append>

