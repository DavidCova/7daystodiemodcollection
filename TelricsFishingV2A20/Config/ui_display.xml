<configs>
<append xpath="/ui_display_info/item_display">	
<item_display_info display_type="FishingPoles" display_group="groupFishingItems">
	<display_entry name="FishingBaseCatchChance" title_key="statFishingBaseCatchChance" display_type="Percent"/>
</item_display_info>
<item_display_info display_type="Bugnet" display_group="groupFishingItems">
	<display_entry name="DegradationMax" title_key="statDegradationMax" tags="perkLivingOffTheLand"/>
</item_display_info>
<item_display_info display_type="FishingBait" display_group="groupFishingItems">
	<display_entry name="FishingCatchChance" title_key="statFishingCatchChance" display_type="Percent" display_leading_plus="true"/>
	<display_entry name="FishingBaitConsumeChance" title_key="statFishingBaitConsumeChance" display_type="Percent" display_leading_plus="true"/>
</item_display_info>

<!-- buff foods -->
<item_display_info display_type="foodFriedShadRoeandBacon" display_group="groupFishingBuffFoods">
	<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
	<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
	<display_entry name="dFortitude" title_key="statShowFortitude" display_leading_plus="true"/>
	<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
	<display_value name="MaxHealthBonus" title_key="statMaxHealthBonus"  display_type="Percent" display_leading_plus="true"/>
</item_display_info>
<item_display_info display_type="foodFriedTrout" display_group="groupFishingBuffFoods">
	<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
	<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
	<display_entry name="dAgility" title_key="statShowAgility" display_leading_plus="true"/>
	<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
	<display_value name="MaxStaminaBonus"  title_key="statMaxStaminaBonus" display_type="Percent" display_leading_plus="true"/>
</item_display_info>
<item_display_info display_type="foodFriedCatfish" display_group="groupFishingBuffFoods">
	<display_value name="Bartering" title_key="statBarteringBonus"  display_type="Percent" display_leading_plus="true"/>
	<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
	<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
	<display_entry name="dIntellect" title_key="statShowIntellect" display_leading_plus="true"/>
	<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
</item_display_info>
<item_display_info display_type="foodMarinatedTunaSteak" display_group="groupFishingBuffFoods">
	<display_entry name="dAdditionalDamage" display_type="Percent" title_key="statEntityDamageMelee"/>
	<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
	<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
	<display_entry name="dStrength" title_key="statShowStrength" display_leading_plus="true"/>
	<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
</item_display_info>
<item_display_info display_type="foodRoastedGrouper" display_group="groupFishingBuffFoods">
	<display_entry name="dAdditionalDamage" display_type="Percent" title_key="statEntityDamageRanged"/>
	<display_entry name="$foodAmountAdd" title_key="statFoodAmount"/>
	<display_entry name="foodHealthAmount" title_key="statHealthAmount"/>
	<display_entry name="dPerception" title_key="statShowPerception" display_leading_plus="true"/>
	<display_entry name="dDuration" title_key="statDuration" display_type="Time"/>
</item_display_info>

</append>
</configs>