<configs>
<!-- adds master buffs -->
<append xpath="/buffs/buff[@name='buffStatusCheck01']">
		<effect_group name="Adds Master Buff">
				<triggered_effect trigger="onSelfBuffUpdate" action="AddBuff" buff="TelricsHealthBarV1Master" target="self" />
		</effect_group>
</append>

