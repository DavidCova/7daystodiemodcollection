<block name="OakMarbleSink">
	<property name="Class" value="Loot"/>
	<property name="CustomIcon" value="vanity2" />
	<property name="DescriptionKey" value="OakMarbleSinkDesc"/>
	<property name="Material" value="Mmetal"/>
	<property name="Shape" value="ModelEntity"/>
	<property name="Path" value="solid"/>
	<property name="Model" value="#@modfolder:Resources/Sinks.unity3d?vanity2"/>
	<property name="Place" value="TowardsPlacer"/>
	<property name="IsDecoration" value="true"/>
	<property name="LootList" value="storageCrate"/>
	<drop event="Harvest" name="terrStone" count="0" tool_category="Disassemble"/>
	<drop event="Harvest" name="resourceMetalPipe" count="2,4" tag="allHarvest"/>
	<drop event="Harvest" name="resourceScrapBrass" count="1,5" tag="WasteTreasuresSinkHarvest"/>
	<drop event="Harvest" name="resourceScrapLead" count="1,5" tag="WasteTreasuresSinkHarvest"/>
	<drop event="Destroy" name="resourceScrapIron" count="5,8"/>
	<drop event="Fall" name="scrapMetalPile" count="1" prob="0.75" stick_chance="1"/>
	<property name="EconomicValue" value="80"/> <!-- disassemble value -->
	<property name="ImposterExchange" value="imposterBlock" param1="116"/>
	<property name="FilterTags" value="MC_building,SC_loot,SC_residential,SC_plumbing"/>
</block>

