<configs>
	<append xpath="/blocks">

 <block name="WaterWell">
	<property name="AllowAllRotations" value="true"/>
        <property name="CustomIcon" value="waterwell" />
		<property name="DescriptionKey" value="WaterWellDesc"/>		
        <property name="Material" value="Mconcrete"/>
        <property name="IsTerrainDecoration" value="true"/>
        <property name="Shape" value="ModelEntity"/>
        <property name="Model" value="#@modfolder:Resources/WaterWells.unity3d?WaterWell"/>       
        <property name="Collide" value="movement,melee"/>
		<property name="MultiBlockDim" value="2,2,2"/>
		<property name="ModelOffset" value="-0.5,0.5,0"/>
        <property name="CreativeMode" value="Player"/>
        <property name="Stacknumber" value="10"/>
        <property name="Group" value="Building,Building2"/>
        <property name="EconomicValue" value="10"/>
        <property name="FilterTags" value="fdecor"/>
        <property name="SortOrder2" value="0050"/>
        <property name="MaxDamage" value="800"/>
		<property name="DisplayInfo" value="Name"/>		
		<property name="Place" value="TowardsPlacer"/>
	<property name="Class" value="PlantGrowing"/>
	<property name="PlantGrowing.FertileLevel" value="1"/>
	<property name="PlantGrowing.IsRandom" value="false"/>
	<property name="PlantGrowing.LightLevelStay" value="0"/>
	<property name="PlantGrowing.LightLevelGrow" value="0"/>
	<drop event="Destroy" count="0"/>
	<drop event="Harvest" name="resourceCrushedSand" count="4,20" tag="allHarvest"/>
	<drop event="Harvest" name="resourceRockSmall" count="55" tag="oreWoodHarvest"/>
</block>

