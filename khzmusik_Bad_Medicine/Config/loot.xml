<configs>
    <!-- Make dirty bandages common, adjust other types -->
    <append xpath="//lootgroup[@name='survivalMedical']">
        <item name="medicalBandageDirty" count="1,2" />
    </append>
    <setattribute xpath="//lootgroup[@name='survivalMedical']/item[@name='medicalBandage']" name="loot_prob_template">low</setattribute>

    <append xpath="//lootgroup[@name='groupMedicalCommon']">
        <item name="medicalBandageDirty" />
    </append>
    <setattribute xpath="//lootgroup[@name='groupMedicalCommon']/item[@name='medicalBandage']" name="loot_prob_template">low</setattribute>

    <append xpath="//lootgroup[@name='groupMedicalUncommon']">
        <item name="medicalBandage" />
    </append>
    <setattribute xpath="//lootgroup[@name='groupMedicalUncommon']/item[@name='medicalFirstAidBandage']" name="loot_prob_template">low</setattribute>

    <append xpath="//lootgroup[@name='groupMedicalRare']">
        <item name="medicalFirstAidBandage" />
    </append>
</configs>
